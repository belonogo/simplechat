#include <QCoreApplication>
#include <QString>
#include <QVector>
#include <QRandomGenerator>
#include <iostream>

QString generateFunnyNickname() {
    QVector<QString> adjectives = {
        "–í–æ–Ω—é—á–∏–π", "–°–º–µ—à–Ω–æ–π", "–ó–ª–æ–±–Ω—ã–π", "–í–µ—Å–µ–ª—ã–π", "–ú–µ–¥–ª–µ–Ω–Ω—ã–π", "–°–æ–Ω–Ω—ã–π", "–ë—ã—Å—Ç—Ä—ã–π", "–õ–µ–Ω–∏–≤—ã–π", "–ì—Ä–æ–º–∫–∏–π", "–¢–∏—Ö–∏–π",
        "–ü—Ä—ã–≥–∞—é—â–∏–π", "–ü—É–≥–∞—é—â–∏–π", "–¢–∞–Ω—Ü—É—é—â–∏–π", "–ö—Ä–∏—á–∞—â–∏–π", "–®—É–º–Ω—ã–π", "–ó–∞–≥–∞–¥–æ—á–Ω—ã–π", "–•–∏—Ç—Ä—ã–π", "–î–æ–±—Ä—ã–π", "–£–≥—Ä—é–º—ã–π", "–ú–∞–ª–µ–Ω—å–∫–∏–π",
        "–ë–æ–ª—å—à–æ–π", "–ú–æ—Ö–Ω–∞—Ç—ã–π", "–ì–ª–∞–¥–∫–∏–π", "–ó–µ–ª–µ–Ω—ã–π", "–°–∏–Ω–∏–π", "–ö—Ä–∞—Å–Ω—ã–π", "–ñ–µ–ª—Ç—ã–π", "–†–æ–∑–æ–≤—ã–π", "–§–∏–æ–ª–µ—Ç–æ–≤—ã–π", "–†—ã–∂–∏–π",
        "–°—Ç—Ä–∞—à–Ω—ã–π", "–ó–∞–±–∞–≤–Ω—ã–π", "–°–µ—Ä–¥–∏—Ç—ã–π", "–°–ø—è—â–∏–π", "–ë–æ–¥—Ä—ã–π", "–£—Å—Ç–∞–≤—à–∏–π", "–ü–æ—Ç–Ω—ã–π", "–ú–æ–∫—Ä—ã–π", "–°—É—Ö–æ–π", "–ü—É—à–∏—Å—Ç—ã–π",
        "–ö–æ–ª—é—á–∏–π", "–ì—Ä—è–∑–Ω—ã–π", "–ß–∏—Å—Ç—ã–π", "–°–∫–æ–ª—å–∑–∫–∏–π", "–®–µ—Ä—à–∞–≤—ã–π", "–ü—Ä–µ–∫—Ä–∞—Å–Ω—ã–π", "–£–∂–∞—Å–Ω—ã–π", "–£–º–Ω—ã–π", "–ì–ª—É–ø—ã–π", "–ú–∞–ª–æ–¥—É—à–Ω—ã–π"
    };

    QVector<QString> nouns = {
        "–ì–æ—Ä–∏–ª–ª–∞", "–ö–æ—Ç", "–ú–µ–¥–≤–µ–¥—å", "–õ–µ–≤", "–ü–∞–Ω–¥–∞", "–ö—Ä–æ–∫–æ–¥–∏–ª", "–ï–Ω–æ—Ç", "–¢–∏–≥—Ä", "–ë–µ–≥–µ–º–æ—Ç", "–ñ–∏—Ä–∞—Ñ",
        "–°–ª–æ–Ω", "–ö—Ä—ã—Å–∞", "–ú—ã—à—å", "–õ–∏—Å–∞", "–í–æ–ª–∫", "–ó–∞—è—Ü", "–ö–µ–Ω–≥—É—Ä—É", "–ö–æ–∞–ª–∞", "–õ–∞–º–∞", "–ë–∞—Ä—Å—É–∫",
        "–ë–µ–ª–∫–∞", "–ö–∞–±–∞–Ω", "–ü–µ—Ç—É—Ö", "–ö—É—Ä–∏—Ü–∞", "–ì—É—Å—å", "–£—Ç–∫–∞", "–û—Ä–µ–ª", "–°–æ–∫–æ–ª", "–°–æ–≤–∞", "–î—è—Ç–µ–ª",
        "–°–≤–∏–Ω—å—è", "–û–≤—Ü–∞", "–ö–æ–∑–∞", "–õ–æ—à–∞–¥—å", "–ö–æ—Ä–æ–≤–∞", "–ë—É–π–≤–æ–ª", "–û—Å–µ–ª", "–í–µ—Ä–±–ª—é–¥", "–ú—É—Ä–∞–≤–µ–π", "–ü—á–µ–ª–∞",
        "–ñ—É–∫", "–ë–∞–±–æ—á–∫–∞", "–°—Ç—Ä–µ–∫–æ–∑–∞", "–ü–∞—É–∫", "–ß–µ—Ä–µ–ø–∞—Ö–∞", "–Ø—â–µ—Ä–∏—Ü–∞", "–ö—Ä–æ–∫–æ–¥–∏–ª", "–ê–∫—É–ª–∞", "–î–µ–ª—å—Ñ–∏–Ω", "–ö–∏—Ç"
    };

QVector<QString> moods = {
        "üòÄ", "üòÅ", "üòÇ", "ü§£", "üòÉ", "üòÑ", "üòÖ", "üòÜ", "üòâ", "üòä",
        "üòã", "üòé", "üòç", "üòò", "üòó", "üòô", "üòö", "‚ò∫Ô∏è", "üôÇ", "ü§ó",
        "ü§©", "ü§î", "ü§®", "üòê", "üòë", "üò∂", "üôÑ", "üòè", "üò£", "üò•",
        "üòÆ", "ü§ê", "üòØ", "üò™", "üò´", "ü•±", "üò¥", "üòå", "üòõ", "üòú",
        "üòù", "ü§§", "üòí", "üòì", "üòî", "üòï", "üôÉ", "ü§ë", "üò≤", "‚òπÔ∏è"
    };

    QString adjective = adjectives.at(QRandomGenerator::global()->bounded(adjectives.size()));
    QString noun = nouns.at(QRandomGenerator::global()->bounded(nouns.size()));
    QString mood = moods.at(QRandomGenerator::global()->bounded(moods.size()));

    QString adjective = adjectives.at(QRandomGenerator::global()->bounded(adjectives.size()));
    QString noun = nouns.at(QRandomGenerator::global()->bounded(nouns.size()));

    return adjective + " " + noun;
}

int main(int argc, char *argv[])
{
    QCoreApplication a(argc, argv);

    QString nickname = generateFunnyNickname();
    std::cout << "Generated nickname: " << nickname.toStdString() << std::endl;

    return a.exec();
}
